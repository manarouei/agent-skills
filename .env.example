# Core service settings
AGENTIC_ENV=development
AGENTIC_LOG_LEVEL=INFO

# Redis settings
AGENTIC_REDIS_URL=redis://localhost:6379/0

# RabbitMQ settings
AGENTIC_RABBITMQ_URL=amqp://guest:guest@localhost:5672//

# Anthropic LLM settings (REQUIRED)
AGENTIC_ANTHROPIC_API_KEY=your-anthropic-api-key-here
AGENTIC_ANTHROPIC_BASE_URL=https://api.anthropic.com
AGENTIC_ANTHROPIC_VERSION=2023-06-01
AGENTIC_ANTHROPIC_DEFAULT_MODEL=claude-3-5-sonnet-20241022

# LLM budget controls
AGENTIC_LLM_MAX_TOKENS_CAP=4096
AGENTIC_LLM_DEFAULT_MAX_COST_USD=1.0

# LLM pricing (optional JSON override)
# AGENTIC_LLM_PRICING_JSON={"claude-3-5-sonnet-20241022":{"input_per_1k":0.003,"output_per_1k":0.015}}

# Runtime limits
AGENTIC_DEFAULT_SKILL_TIMEOUT_S=30
AGENTIC_DEFAULT_AGENT_STEP_LIMIT=10

# Celery settings
AGENTIC_CELERY_TASK_TIME_LIMIT=300
AGENTIC_CELERY_TASK_SOFT_TIME_LIMIT=270
