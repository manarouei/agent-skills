      "operation": "getRepository",
      "parameters": {
        "owner": "n8n-io",
        "repository": "n8n"
      },
      "credentials": {
        "githubApi": "github-test-credential"
      },
      "input_data": null,
      "expected_output": {
        "type": "object",
